<?xml version="1.0" encoding="utf-8"?>

<xs:schema id="ChartModelSchema"
           targetNamespace="http://schemas.iTin.com/charting/chart/v1.0"
           elementFormDefault="qualified"
           xmlns="http://schemas.iTin.com/charting/chart/v1.0"
           xmlns:mstns="http://schemas.iTin.com/charting/chart/v1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           version="1.0">

  <!-- Base types -->
  <xs:include schemaLocation="ModelSharedTypes.xsd" />

  <!-- Root -->
  <xs:element name="Chart" type="ChartModel">
    <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
      <xs:documentation xml:lang="en">
        Represents a user-defined chart
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <!-- Root: Chart -->
  <xs:complexType name="ChartModel" id="ChartModel">
    <xs:all>
      <xs:element name="Axes" type="AxesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of chart axes
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Border" type="BorderModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of chart border
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FooterTitle" type="FooterTitleModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of chart footer title
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Legend" type="LegendModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of chart legend
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Plots" type="PlotsModel" minOccurs="1" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of chart footer title
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Size" type="SizeModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines size of chart
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Title" type="TitleModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of chart title
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Quality" type="QualityModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines quality of chart
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="BackColor" type="rgbColorType" use="optional" default="White">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred background color for the chart. The default is 'White'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: Axes -->
  <xs:complexType name="AxesModel" id="AxesModel">
    <xs:all>
      <xs:element name="Primary" type="AxisModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines the information about the primary axes. Includes information for the horizontal and vertical axis
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Secondary" type="AxisModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines the information about the secondary axes. Includes information for the horizontal and vertical axis
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <!-- Chart: Border -->
  <xs:complexType name="BorderModel" id="BorderModel">
    <xs:all>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Shadow" type="ShadowModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of border's shadow. Includes the shadow color and its visibility
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Color" type="rgbColorType" use="optional" default="Black">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred border color. The default is 'Black'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Style" type="KnownLineStyle" use="optional" default="Continuous">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred border line style. The default is 'Continuous'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Show" type="YesNo" use="optional" default="No">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Determines if the border is displayed. The default is 'No'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Width" type="intMinValueIsOneType" use="optional" default="1">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred width of the edge line. The default is '1'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: FooterTitle -->
  <xs:complexType name="FooterTitleModel" id="FooterTitleModel">
    <xs:all>
      <xs:element name="Alignment" type="ContentAlignmentModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines content distribution
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Border" type="BorderModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of footer title border
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Font" type="FontModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines font to use
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="BackColor" type="rgbColorType" use="optional" default="White">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred background color. The default is 'White'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Orientation" type="KnownTextOrientation" use="optional" default="Auto">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred orientation for the footer title. The default is 'Auto'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Text" type="freeStringType" use="optional" default="">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred footer title text. The default is ''.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Show" type="YesNo" use="optional" default="No">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Determines if the footer is displayed. The default is 'No'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: Legend -->
  <xs:complexType name="LegendModel" id="LegendModel">
    <xs:all>
      <xs:element name="Border" type="BorderModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of legend border
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Font" type="FontModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines font to use
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Alignment" type="KnownTextAlignment" use="optional" default="Center">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred text alignment. The default is 'Center'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="BackColor" type="rgbColorType" use="optional" default="White">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred background color for chart legend. The default is 'White'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Location" type="KnownLegendLocation" use="optional" default="Bottom">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred location for chart legend. The default is 'Bottom'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Show" type="YesNo" use="optional" default="No">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Determines if the legend is displayed. The default is 'No'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: Plots -->
  <xs:complexType name="PlotsModel" id="PlotsModel">
    <xs:sequence>
      <xs:element name="Border" type="BorderModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Visual setting for common border of plots.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PlotAreas" type="PlotAreasModel" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Collection of plot areas for a chart. Each element represents a chart plot area
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="BackColor" type="rgbColorType" use="optional" default="White">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred back color. The default is 'White'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Show" type="YesNo" use="optional" default="Yes">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Determines if chart plot area is displayed. The default is 'Yes'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: Size -->
  <xs:complexType name="SizeModel" id="SizeModel">
    <xs:all>
      <xs:element name="CustomSize" type="CustomSizeModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a custom size
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Resolution" type="xs:string" use="optional" default="SXGA">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred chart resolution. The default is 'SXGA'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: Title -->
  <xs:complexType name="TitleModel" id="TitleModel">
    <xs:all>
      <xs:element name="Alignment" type="ContentAlignmentModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines content distribution
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Border" type="BorderModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of title border
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Font" type="FontModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines font to use
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="BackColor" type="rgbColorType" use="optional" default="White">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred background color. The default is 'White'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Orientation" type="KnownTextOrientation" use="optional" default="Auto">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred orientation for the title. The default is 'Auto'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Text" type="freeStringType" use="optional" default="">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred title text. The default is ''.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Show" type="YesNo" use="optional" default="Yes">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Determines if the title is displayed. The default is 'Yes'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- Chart: Quality -->
  <xs:complexType name="QualityModel" id="QualityModel">
    <xs:all>
      <xs:element name="CustomQuality" type="CustomQualityModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a custom chart quality
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Quality" type="xs:string" use="optional" default="High">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred chart quality. The default is 'High'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <!-- Axes: Axis -->
  <xs:complexType name="AxisModel" id="AxisModel">
    <xs:all>
      <xs:element name="Horizontal" type="AxisDefinitionModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of horizontal axis. Allows you to configure the axis title, the axis labels, axis marks and axis values.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Vertical" type="AxisDefinitionModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of vertical axis. Allows you to configure the axis title, the axis labels, axis marks and axis values.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <!-- Axis: AxisDefinition -->
  <xs:complexType name="AxisDefinitionModel" id="AxisDefinitionModel">
    <xs:all>
      <xs:element name="Grid" type="AxisGridModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of axis grid
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Labels" type="AxisLabelsModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of axis grid
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Marks" type="AxisMarksModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of axis marks
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Title" type="AxisTitleModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual settings of axis title
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Values" type="AxisValuesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of axis values
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="LineColor" type="rgbColorType" use="optional" default="Black">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred color of shaft line. The default is 'Black'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="LineDashStyle" type="KnownLineStyle" use="optional" default="Continuous">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred line style of axis. The default is 'Continuous'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="LineWidth" type="intMinValueIsOneType" use="optional" default="1">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Preferred width of axis line. The default is '1'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Name" type="freeStringType" use="optional" default="">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Axis name. The default is ''.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Show" type="YesNo" use="optional" default="Yes">
      <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
        <xs:documentation xml:lang="en">
          [Optional] Determines if the axis is displayed. The default is 'Yes'.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <!-- AxisDefinition: Grid -->
  <xs:complexType name="AxisGridModel" id="AxisGridModel">
    <xs:all>
      <xs:element name="MajorGrid" type="AxisGridDefinitionModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of major grid
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MinorGrid" type="AxisGridDefinitionModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines visual setting of minor grid
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Properties" type="PropertiesModel" minOccurs="0" maxOccurs="1">
        <xs:annotation xmlns="http://www.w3.org/2001/XMLSchema">
          <xs:documentation xml:lang="en">
            Defines a set of custom properties. List of keys item with name / value
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <!-- AxisDefinition: Marks -->
  <xs:complexType name="AxisMarksModel" id="AxisMarksModel">
    <xs:all>
