      "type": "object",
      "properties": {
        "Properties": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/PropertyModel"
          }
        }
      }
    },
    "FooterTitleModel": {
      "allOf": [
        {
          "$ref": "#/definitions/BaseModelOfTitleModel"
        },
        {
          "type": "object",
          "description": "Represents the visual setting of bottom title. Includes a text, visibility, orientation, border and font",
          "properties": {
            "BackColor": {
              "type": "string",
              "description": "Gets or sets preferred back color for this chart title. The default is 'White'",
              "default": "White"
            },
            "Border": {
              "description": "Gets or sets a reference that contains the visual setting for border of title",
              "oneOf": [
                {
                  "$ref": "#/definitions/BorderModel"
                }
              ]
            },
            "Font": {
              "description": "Gets or sets a reference to the font model defined for this title",
              "oneOf": [
                {
                  "$ref": "#/definitions/FontModel"
                }
              ]
            },
            "Show": {
              "description": "Gets or sets a value that determines whether displays the title. The default is 'No'",
              "default": "No",
              "oneOf": [
                {
                  "$ref": "#/definitions/YesNo"
                }
              ]
            },
            "Text": {
              "type": "string",
              "description": "Gets or sets text of title. The default is ''",
              "default": ""
            },
            "Alignment": {
              "description": "Gets or sets preferred alignment of bottom chart title",
              "oneOf": [
                {
                  "$ref": "#/definitions/ContentAlignmentModel"
                }
              ]
            },
            "Orientation": {
              "description": "Gets or sets the orientation of the text in the axis title. The default value is 'Auto'",
              "default": "Auto",
              "oneOf": [
                {
                  "$ref": "#/definitions/KnownTextOrientation"
                }
              ]
            }
          }
        }
      ]
    },
    "ContentAlignmentModel": {
      "allOf": [
        {
          "$ref": "#/definitions/BaseModelOfContentAlignmentModel"
        },
        {
          "type": "object",
          "description": "Defines content alignment.",
          "properties": {
            "Horizontal": {
              "description": "Gets or sets horizontal alignment. The default is 'Center'",
              "default": "Center",
              "oneOf": [
                {
                  "$ref": "#/definitions/KnownHorizontalAlignment"
                }
              ]
            },
            "Vertical": {
              "description": "Gets or sets vertical alignment. The default is 'Top'",
              "default": "Top",
              "oneOf": [
                {
                  "$ref": "#/definitions/KnownVerticalAlignment"
                }
              ]
            }
          }
        }
      ]
    },
    "KnownHorizontalAlignment": {
      "type": "string",
      "description": "Specifies how an object or text is horizontally aligned relative to a content",
      "x-enumNames": [
        "Left",
        "Center",
        "Right"
      ],
      "enum": [
        "Left",
        "Center",
        "Right"
